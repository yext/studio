{
  "name": "@yext/sample-component",
  "private": true,
  "version": "0.0.0",
  "main": "./lib/cjs/index.js",
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./virtual-module": {
      "types": "./virtual-module.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch --preserveWatchOutput -p tsconfig.node.json & tsc --watch --preserveWatchOutput -p tsconfig.json",
    "build": "rm -rf lib && tsc -p tsconfig.node.json && tsc -p tsconfig.json",
    "lint": "eslint . && npm run prettier-check",
    "prettier-write": "prettier --write .",
    "prettier-check": "prettier --check ."
  },
  "dependencies": {
    "react": "^18.2.0",
    "typescript": "^4.9.4",
    "@yext/studio": "*"
  }
}