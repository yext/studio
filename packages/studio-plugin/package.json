{
  "name": "@yext/studio-plugin",
  "version": "0.3.0",
  "types": "./lib/esm/index.d.ts",
  "main": "./lib/esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index-cjs.js"
    },
    "./virtual-module": {
      "types": "./virtual-module.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "tsc --watch --preserveWatchOutput -p tsconfig.esm.json & tsc --watch --preserveWatchOutput -p tsconfig.json >/dev/null",
    "build": "rm -rf lib && tsc -p tsconfig.esm.json && tsc -p tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@yext/pages": "^1.0.0-beta.14",
    "prettier": "^2.8.1",
    "react-dev-utils": "^12.0.1",
    "ts-morph": "^17.0.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/react-dev-utils": "^9.0.11",
    "@types/uuid": "^9.0.0",
    "jest": "^29.3.1",
    "react": "^18.2.0",
    "vite": "^4.0.4",
    "webpack": "^5.75.0"
  }
}
